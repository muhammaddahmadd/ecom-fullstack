{
  "name": "ecom-express-server",
  "version": "1.0.0",
  "description": "Node.js Express server with TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "clean": "rm -rf dist",
    "seed": "ts-node src/scripts/seedDatabase.ts",
    "db:seed": "ts-node src/scripts/seedDatabase.ts",
    "db:reset": "ts-node -e \"require('./src/scripts/seedDatabase.ts').default()\""
  },
  "keywords": [
    "express",
    "typescript",
    "node",
    "server"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "mongoose": "^8.17.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
